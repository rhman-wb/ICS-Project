{
    "compilerOptions":  {
                            "target":  "ES2020",
                            "lib":  [
                                        "ES2020",
                                        "DOM",
                                        "DOM.Iterable"
                                    ],
                            "module":  "ESNext",
                            "skipLibCheck":  true,
                            "moduleResolution":  "bundler",
                            "allowImportingTsExtensions":  true,
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "noEmit":  true,
                            "jsx":  "preserve",
                            "strict":  true,
                            "noUnusedLocals":  false,
                            "noUnusedParameters":  false,
                            "noFallthroughCasesInSwitch":  true,
                            "baseUrl": "..",
                            "paths":  {
                                          "@/*":  [
                                                      "src/*"
                                                  ],
                                          "@components/*":  [
                                                                "src/components/*"
                                                            ],
                                          "@views/*":  [
                                                           "src/views/*"
                                                       ],
                                          "@utils/*":  [
                                                           "src/utils/*"
                                                       ],
                                          "@api/*":  [
                                                         "src/api/*"
                                                     ],
                                          "@stores/*":  [
                                                            "src/stores/*"
                                                        ],
                                          "@assets/*":  [
                                                            "src/assets/*"
                                                        ]
                                      },
                            "types":  [
                                          "vite/client",
                                          "node"
                                      ]
                        },
    "files":  [
                  "../src/api/modules/audit.ts",
                  "../src/api/types/audit.ts",
                  "../src/api/mock/audit/index.ts",
                  "../src/api/mock/audit/auditResults.ts",
                  "../src/api/mock/audit/auditRules.ts",
                  "../src/composables/audit/useAuditResults.ts",
                  "../src/composables/audit/useAuditProgress.ts",
                  "../src/composables/audit/useAuditRules.ts"
              ]
}

